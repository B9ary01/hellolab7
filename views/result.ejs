 <h1> This is User Search Page </h1>  
<body>
<h3>You can search the registered users here:</h3>
  <p>Please enter username below:</p>
<form action="registereduser" method="GET">                                                                             
<input type="text" id="searchuserName"                                                                              
  class="form-control" name="username" placeholder="enter username" required>
</form>

<script>
$(function(){
$('#searchuserName').autocomplete({
source: function(req,res){
$.ajax({
url:"registereduser/",dataType:"jsonp",
type:"GET",
data:req, 
success: function(data){
res(data) },error: function(err){
console.log(err.status);         }
});     },
minLength:1,
select: function(event,ui){
if(ui.item){
$('#searchuserName').val(ui.item.label);
}     }     });       });
</script>
</body>
</html>
